{
  "name": "snitch",
  "version": "1.0.0",
  "description": "Monitor what you'd like ;)",
  "main": "serverless.ts",
  "scripts": {
    "start:local": "serverless invoke local --function",
    "deploy": "export $(grep -v '^#' .env | xargs) && sls deploy",
    "print": "export $(grep -v '^#' .env | xargs) && sls print"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@middy/core": "^3.6.0",
    "@middy/http-json-body-parser": "^3.6.0",
    "cheerio": "^1.0.0-rc.12"
  },
  "devDependencies": {
    "@grammyjs/types": "^2.9.1",
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/node": "^14.14.25",
    "esbuild": "^0.14.11",
    "json-schema-to-ts": "^1.5.0",
    "serverless": "^3.22.0",
    "serverless-dotenv-plugin": "^4.0.2",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-esbuild": "^1.23.3",
    "serverless-offline": "^11.1.3",
    "serverless-ssm-publish": "^1.4.2",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "author": "nodm",
